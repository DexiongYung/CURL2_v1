                "--actor_lr", "1e-3", 
                "--eval_freq", "1000",
                "--batch_size", "512", 
                "--num_train_steps", "200000",
                "--save_tb",
                "--save_video",
                "--save_model",
                "--log_interval", "1",
                "--config_file", "./configs/paper_hyperparam.json"
